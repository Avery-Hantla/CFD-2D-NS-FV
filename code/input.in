[case]
    nmax = 10000
    monitor_step = 50
    output_step = 1000
    solution_order = 1
    restart = 0 

[grid]
    grid_file = ../grids/grid_file.in 

[equations] 
    eqn = NS
    flux_solver = RUSANOV

[fluid]
    gamma = 1.4
    R = 287
    mu = 0.0000178
    Pr = 0.72

[init]
    P = 1
    rho = 1.4
    u = 0.85
    v = 0

[time]
    time_scheme = SSP_RK3
    CFL = 1

[limiter]
    limiter = NONE

[report]
    length = 1 

[BC]
    BC1 = SLIP_WALL
    BC2 = FREESTREAM