[case]
    nmax = 10000
    monitor_step = 50
    output_step = 1000
    solution_order = 2
    restart = 0 

[grid]
    grid_file = ../grids/grid_file.in 

[equations] 
    eqn = EULER
    flux_solver = RUSANOV

[flow]
    gamma = 1.4
    P = 1
    rho = 1.4
    u = 0.85
    v = 0

[time]
    time_scheme = SSP_RK3
    CFL = 1

[limiter]
    limiter = SQUEEZE

[report]
    u = 0.85
    v = 0
    P = 1 
    rho = 1.4 
    area = 1
    length = 1 

[BC]
    BC1 = SLIP_WALL
    BC2 = FREESTREAM